<!DocumentType html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Chicken Taco Recipe</title>
</head>
<body>
    <h1><%= recipe.name %></h1>
    <h2>Ingredients:</h2>
    <ul>
        <% recipe.ingredients.forEach(function(ingredient) { %>
            <li><%= ingredient %></li>
        <% }); %>
    </ul>
    <h2>Instructions:</h2>
    <ol>
        <% recipe.instructions.forEach(function(step) { %>
            <li><%= step %></li>
        <% }); %>
    </ol>
</body>


</html>